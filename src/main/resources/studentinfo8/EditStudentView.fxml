<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<fx:root depthTest="ENABLE" spacing="5.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Name:" />
    <TextField fx:id="nameField" blendMode="SRC_ATOP" promptText="输入名字">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
    <Label text="Class:" />
    <TextField fx:id="classField" />
    <Label text="Address:" />
    <TextField fx:id="addressField" />
    <Button fx:id="saveButton" onAction="#onSaveClick" text="Save" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>


</fx:root>
