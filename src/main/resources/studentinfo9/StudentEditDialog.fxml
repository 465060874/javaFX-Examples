<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>

<GridPane fx:controller="com.demo.javafxexample.apps.studentinfo9.StudentEditDialogController"
          xmlns:fx="http://javafx.com/fxml"
          alignment="CENTER"
          hgap="10.0"
          vgap="10.0">
    <Label text="Name:" GridPane.rowIndex="0" />
    <TextField fx:id="nameField" GridPane.rowIndex="0" GridPane.columnIndex="1" />
    <Label text="Class:" GridPane.rowIndex="1" />
    <TextField fx:id="classField" GridPane.rowIndex="1" GridPane.columnIndex="1" />
    <Label text="Address:" GridPane.rowIndex="2" />
    <TextField fx:id="addressField" GridPane.rowIndex="2" GridPane.columnIndex="1" />
    <Button text="Save" fx:id="saveButton" onAction="#handleSave" GridPane.rowIndex="3" GridPane.columnIndex="1" />
</GridPane>
